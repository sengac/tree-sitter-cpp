{
  "name": "@sengac/tree-sitter-cpp",
  "version": "0.25.5",
  "description": "C++ grammar for tree-sitter",
  "repository": "https://github.com/sengac/tree-sitter-cpp",
  "license": "MIT",
  "author": {
    "name": "Max Brunsfeld",
    "email": "maxbrunsfeld@gmail.com"
  },
  "maintainers": [
    {
      "name": "Amaan Qureshi",
      "email": "amaanq12@gmail.com"
    }
  ],
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "c++"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2",
    "@sengac/tree-sitter-c": "^0.25.6"
  },
  "devDependencies": {
    "eslint": "^9.12.0",
    "eslint-config-treesitter": "^1.0.2",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.0"
  },
  "peerDependencies": {
    "@sengac/tree-sitter": "^0.25.10"
  },
  "peerDependenciesMeta": {
    "@sengac/tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "lint": "eslint grammar.js",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "install": "node-gyp-build"
  },
  "publishConfig": {
    "access": "public"
  }
}
